.home {
  height: 100vh;
  position: relative;
}
.home .header-bg {
  position: relative;
  z-index: -3;
  width: 100%;
  top: 0;
}
.home .about {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 31rpx;
  overflow-y: auto;
  padding-bottom: 120rpx;
}
.home .about .header {
  padding: 180rpx 31rpx 40rpx;
  position: relative;
  z-index: -2;
}
.home .about .header .user-block {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.home .about .header .user-block .user {
  display: flex;
  justify-content: start;
  align-items: center;
}
.home .about .header .user-block .user .user-name {
  padding-left: 20rpx;
  color: #FFFFFF;
}
.home .about .header .user-block .user .user-name .name-text {
  font-size: 48rpx;
}
.home .about .header .user-block .user .user-name .name-tag {
  font-size: 22rpx;
  margin-top: 10rpx;
}
.home .about .header .user-block .header-img {
  width: 187rpx;
  height: 156rpx;
  display: block;
  object-fit: cover;
}
.home .about .liststyle {
  position: relative;
  z-index: 1;
}
.home .about .liststyle::after {
  content: "";
  z-index: 1;
  display: block;
  height: 10rpx;
  width: 100%;
  background: #333;
  box-shadow: 0 5rpx 15rpx #3974F4;
  border-radius: 10rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.home .about .liststyle .content {
  position: relative;
  z-index: 1;
  width: 98%;
  background: #FFFFFF;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 20rpx;
}
.home .about .liststyle .content .content-type {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.home .about .liststyle .content .content-type .type-left {
  width: 120rpx;
  flex-shrink: 0;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 26rpx;
  border-left: 4rpx solid #373737;
  padding-left: 20rpx;
}
.home .about .liststyle .content .content-type .type-num {
  width: 97rpx;
  height: 97rpx;
  font-size: 32rpx;
  font-weight: 600;
  box-sizing: border-box;
  border-radius: 50%;
  box-shadow: 0 0 20rpx #ccc;
  padding: 8rpx;
  flex-shrink: 0;
}
.home .about .liststyle .content .content-type .type-num .problem-num {
  border: 5rpx solid #3974F4;
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 71rpx;
  border-radius: 50%;
  box-sizing: border-box;
}
.home .about .liststyle .content .content-type .correct-num {
  width: 120rpx;
  flex-shrink: 0;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 26rpx;
}
.home .about .liststyle .content .content-problem .p-title {
  font-size: 26rpx;
  font-weight: normal;
  color: #373737;
  line-height: 40rpx;
  margin-top: 24rpx;
}
.home .about .liststyle .content .content-problem .p-list {
  padding-bottom: 24rpx;
  border-bottom: 1rpx dashed #ccc;
}
.home .about .liststyle .content .content-problem .p-list .uni-data-checklist .checklist-group {
  display: block;
}
.home .about .liststyle .content .content-problem .p-list .uni-data-checklist .checklist-group .checklist-group {
